---
import Start from '../layouts/Start.astro';
import "../styles/global.css";

import amarre_img from "../assets/slider/amarre.jpeg";
import amarres_lov_img from "../assets/slider/amarresdeamor.jpeg";
import tabaco_img from "../assets/slider/tabaco.jpg";
import tarot_img from "../assets/slider/tarot.jpg";

import copas_img from "../assets/service/copas.jpg";
import love_img from "../assets/service/love2.jpg";
import spirit_img from "../assets/service/spiritu.jpg";
import tarot_img2 from "../assets/service/tarot2.jpg";
import vela_img from "../assets/service/vela.jpg";

import whatsapp_icon from "../assets/whatsapp.png";
import hechizos_img from "../assets/secctions/hechizos.jpg"
import lectura_img from "../assets/about/lecturatabaco.jpeg"

import family_img from "../assets/slider_buttom/family.jpg"
import hands_img from "../assets/slider_buttom/hands.jpg"
import pasion_img from "../assets/slider_buttom/pasion.jpg"

import pedestal_img from "../assets/contact/pedestal.jpeg"
import pedestal_img2 from "../assets/contact/pedestal2.jpeg"
---

<Start>

	<header class="bg-black dark:bg-black">
		<div class="mx-auto max-w-screen-xl px-4 sm:px-6 lg:px-8">
		  <div class="flex h-16 items-center justify-between">
			<div class="md:flex md:items-center md:gap-12">
			  <a class="block text-teal-600 dark:text-teal-600" href="#">
				<span class="sr-only">Home</span>
				<h2 class="font-bold">Prosperidad y union</h2>
			  </a>
			</div>
	  
			<div class="hidden md:block">
			  <nav aria-label="Global">
				<ul class="flex items-center gap-6 text-sm">
				  <li>
					<a
					class="group inline-block rounded-full bg-gradient-to-r from-rose-500 via-rose-500 to-yellow-500 p-[2px] hover:text-white focus:ring-3 focus:outline-hidden text-gray-500 transition hover:text-gray-500/75 dark:text-white dark:hover:text-white/75"
					href="#imagen-fondo"
					>
					<span
						class="block rounded-full bg-black px-8 py-3 text-sm font-medium group-hover:bg-transparent"
					>
						Inicio
					</span>
					</a>

				  </li>
	  
				  <li>
					<a
					  class="text-gray-500 transition-all duration-300 ease-in-out hover:text-lg dark:text-gray-300 dark:hover:text-white/75"
					  href="#sec-nosotros"
					>
					  Nosotros
					</a>
				  </li>
								
	  
				  <li>
					<a
					  class="text-gray-500 transition-all duration-300 ease-in-out hover:text-lg dark:text-gray-300 dark:hover:text-white/75"
					  href="#sec-servicios"
					>
					  Servicios
					</a>
				  </li>
	  
				  <li>
					<a
					  class="text-gray-500 transition-all duration-300 ease-in-out hover:text-lg dark:text-gray-300 dark:hover:text-white/75"
					  href="#sec-contact"
					>
					  Contactenos
					</a>
				  </li>
				</ul>
			  </nav>
			</div>
	  
			<div class="flex items-center gap-4">
	  
			  <div class="block md:hidden">



				<div class="relative">
					<!-- Botón con funcionalidad de abrir/cerrar el menú -->
					<button id="menuButton" class="rounded-sm bg-gray-100 p-2 text-gray-600 transition hover:text-gray-600/75 dark:bg-gray-800 dark:text-white dark:hover:text-white/75">
					  <svg xmlns="http://www.w3.org/2000/svg" class="size-5" fill="none" viewBox="0 0 24 24" stroke="currentColor" stroke-width="2">
						<path stroke-linecap="round" stroke-linejoin="round" d="M4 6h16M4 12h16M4 18h16" />
					  </svg>
					</button>
				  
