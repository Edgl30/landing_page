							<div>
							  <a href="#imagen-fondo" class="font-semibold text-gray-500">
								Inicio
								<span class="absolute inset-0"></span>
							  </a>
							</div>
						  </div>
				  
						  <div class="group relative flex gap-x-6 rounded-lg p-4 hover:bg-gray-300">
							<div>
